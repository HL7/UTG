    },
    {
      "name" : "version",
      "valueUri" : "http://terminology.hl7.org/CodeSystem/v3-ActReason|2.1.0"
    }],
    "contains" : [{
      "extension" : [{
        "url" : "http://hl7.org/fhir/StructureDefinition/valueset-definition",
        "valueString" : "**Description:**This therapy has been ordered as a backup to a preferred therapy. This order will be released when and if the preferred therapy is unsuccessful."
      }],
      "system" : "http://terminology.hl7.org/CodeSystem/v3-ActReason",
      "code" : "ALTCHOICE",
      "display" : "try another treatment first"
    },
    {
      "extension" : [{
        "url" : "http://hl7.org/fhir/StructureDefinition/valueset-definition",
        "valueString" : "**Description:**Clarification is required before the order can be acted upon."
      }],
      "system" : "http://terminology.hl7.org/CodeSystem/v3-ActReason",
      "code" : "CLARIF",
      "display" : "prescription requires clarification"
    },
    {
      "extension" : [{
        "url" : "http://hl7.org/fhir/StructureDefinition/valueset-definition",
        "valueString" : "**Description:**The current level of the medication in the patient's system is too high. The medication is suspended to allow the level to subside to a safer level."
      }],
      "system" : "http://terminology.hl7.org/CodeSystem/v3-ActReason",
      "code" : "DRUGHIGH",
      "display" : "drug level too high"
    },
    {
      "extension" : [{
        "url" : "http://hl7.org/fhir/StructureDefinition/valueset-definition",
        "valueString" : "**Description:**The patient has been admitted to a care facility and their community medications are suspended until hospital discharge."
      }],
      "system" : "http://terminology.hl7.org/CodeSystem/v3-ActReason",
      "code" : "HOSPADM",
      "display" : "admission to hospital"
    },
    {
      "extension" : [{
        "url" : "http://hl7.org/fhir/StructureDefinition/valueset-definition",
        "valueString" : "**Description:**The therapy would interfere with a planned lab test and the therapy is being withdrawn until the test is completed."
      }],
      "system" : "http://terminology.hl7.org/CodeSystem/v3-ActReason",
      "code" : "LABINT",
      "display" : "lab interference issues"
    },
    {
      "extension" : [{
        "url" : "http://hl7.org/fhir/StructureDefinition/valueset-definition",
        "valueString" : "**Description:**Patient not available for a period of time due to a scheduled therapy, leave of absence or other reason."
      }],
      "system" : "http://terminology.hl7.org/CodeSystem/v3-ActReason",
      "code" : "NON-AVAIL",
      "display" : "patient not-available"
    },
    {
      "extension" : [{
        "url" : "http://hl7.org/fhir/StructureDefinition/valueset-definition",
        "valueString" : "**Description:**The patient is pregnant or breast feeding. The therapy will be resumed when the pregnancy is complete and the patient is no longer breastfeeding."
      }],
      "system" : "http://terminology.hl7.org/CodeSystem/v3-ActReason",
      "code" : "PREG",
      "display" : "parent is pregnant/breast feeding"
    },
    {
      "extension" : [{
        "url" : "http://hl7.org/fhir/StructureDefinition/valueset-definition",
        "valueString" : "**Description:**The patient is believed to be allergic to a substance that is part of the therapy and the therapy is being temporarily withdrawn to confirm."
      }],
      "system" : "http://terminology.hl7.org/CodeSystem/v3-ActReason",
      "code" : "SALG",
      "display" : "allergy"
    },
    {
      "extension" : [{
        "url" : "http://hl7.org/fhir/StructureDefinition/valueset-definition",
        "valueString" : "**Description:**The drug interacts with a short-term treatment that is more urgently required. This order will be resumed when the short-term treatment is complete."
      }],
      "system" : "http://terminology.hl7.org/CodeSystem/v3-ActReason",
      "code" : "SDDI",
      "display" : "drug interacts with another drug"
    },
    {
      "extension" : [{
        "url" : "http://hl7.org/fhir/StructureDefinition/valueset-definition",
        "valueString" : "**Description:**Another short-term co-occurring therapy fulfills the same purpose as this therapy. This therapy will be resumed when the co-occuring therapy is complete."
      }],
      "system" : "http://terminology.hl7.org/CodeSystem/v3-ActReason",
      "code" : "SDUPTHER",
      "display" : "duplicate therapy"
    },
    {
      "extension" : [{
        "url" : "http://hl7.org/fhir/StructureDefinition/valueset-definition",
        "valueString" : "**Description:**The patient is believed to have an intolerance to a substance that is part of the therapy and the therapy is being temporarily withdrawn to confirm."
      }],
      "system" : "http://terminology.hl7.org/CodeSystem/v3-ActReason",
      "code" : "SINTOL",
      "display" : "suspected intolerance"
    },
    {
      "extension" : [{
        "url" : "http://hl7.org/fhir/StructureDefinition/valueset-definition",
        "valueString" : "**Description:**The drug is contraindicated for patients receiving surgery and the patient is scheduled to be admitted for surgery in the near future. The drug will be resumed when the patient has sufficiently recovered from the surgery."
      }],
      "system" : "http://terminology.hl7.org/CodeSystem/v3-ActReason",
      "code" : "SURG",
