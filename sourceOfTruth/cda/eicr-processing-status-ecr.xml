<?xml version="1.0" encoding="UTF-8"?>

<ValueSet xmlns="http://hl7.org/fhir">
  <id value="eicr-processing-status-ecr"/>
  <meta id="4385"/>
  <url value="http://hl7.org/fhir/us/ecr/ValueSet/eicr-processing-status-ecr"/>
  <name value="eICR Processing Status (eCR)"/>
  <status value="active"/>
  <description value="This value set contains codes that describe the status of the eICR document in terms of processing by the public health decision support system."/>
  <compose>
    <include>
      <system value="http://terminology.hl7.org/CodeSystem/2.16.840.1.114222.4.5.274"/>
      <concept>
        <code value="RRVS20"/>
        <display value="eICR was processed - with a warning"/>
      </concept>
      <concept>
        <code value="RRVS19"/>
        <display value="eICR processed"/>
      </concept>
      <concept>
        <code value="RRVS22"/>
        <display value="eICR was not processed - error"/>
      </concept>
      <concept>
        <code value="RRVS21"/>
        <display value="eICR was processed - with a severe warning"/>
      </concept>
    </include>
  </compose>
</ValueSet>