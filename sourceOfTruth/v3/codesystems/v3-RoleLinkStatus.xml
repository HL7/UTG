<?xml version="1.0" encoding="UTF-8"?>

<CodeSystem xmlns="http://hl7.org/fhir">
  <id value="v3-RoleLinkStatus"/>
  <language value="en"/>
  <text>
    <status value="generated"/>
    <div xmlns="http://www.w3.org/1999/xhtml">
                  
      <p>
                     
        <b>Description:</b>Codes representing possible states of a RoleLink, as defined by the RoleLink class state machine.
      </p>
               
    </div>
  </text>
  <url value="http://terminology.hl7.org/CodeSystem/v3-RoleLinkStatus"/>
  <identifier>
    <system value="urn:ietf:rfc:3986"/>
    <value value="urn:oid:2.16.840.1.113883.5.1137"/>
  </identifier>
  <name value="RoleLinkStatus"/>
  <title value="RoleLinkStatus"/>
  <status value="active"/>
  <date value="2017-07-31"/>
  <publisher value="HL7 International"/>
  <contact>
    <name value="Health Level Seven"/>
  </contact>
  <description value="  Description:&#xD;&#xA;Codes representing possible states of a RoleLink, as defined by the RoleLink class state machine."/>
  <content value="complete"/>
  <concept>
    <code value="NORMAL"/>
    <display value="normal"/>
    <definition value="  Description:&#xD;&#xA;The &#39;typical&#39; state. Excludes &quot;nullified&quot; which represents the termination state of a RoleLink instance that was created in error."/>
    <property>
      <code value="status"/>
      <valueCode value="active"/>
    </property>
    <concept>
      <code value="ACTIVE"/>
      <display value="active"/>
      <definition value="  Description:&#xD;&#xA;The state indicates the RoleLink is in progress."/>
      <property>
        <code value="status"/>
        <valueCode value="active"/>
      </property>
    </concept>
    <concept>
      <code value="CANCELLED"/>
      <display value="cancelled"/>
      <definition value="  Description:&#xD;&#xA;The terminal state resulting from abandoning the RoleLink prior to or after activation."/>
      <property>
        <code value="status"/>
        <valueCode value="active"/>
      </property>
    </concept>
    <concept>
      <code value="COMPLETED"/>
      <display value="completed"/>
      <definition value="  Description:&#xD;&#xA;The terminal state representing the successful completion of the RoleLink."/>
      <property>
        <code value="status"/>
        <valueCode value="active"/>
      </property>
    </concept>
    <concept>
      <code value="PENDING"/>
      <display value="pending"/>
      <definition value="  Description:&#xD;&#xA;The state indicates the RoleLink has not yet become active."/>
      <property>
        <code value="status"/>
        <valueCode value="active"/>
      </property>
    </concept>
  </concept>
  <concept>
    <code value="NULLIFIED"/>
    <display value="nullified"/>
    <definition value="  Description:&#xD;&#xA;The state representing the termination of a RoleLink instance that was created in error."/>
    <property>
      <code value="status"/>
      <valueCode value="active"/>
    </property>
  </concept>
</CodeSystem>