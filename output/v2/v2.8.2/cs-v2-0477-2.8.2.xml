    </property>
  </concept>
  <concept id="3">
    <extension url="http://hl7.org/fhir/StructureDefinition/codesystem-comments">
      <valueString value="Includes drugs having an abuse potential less than that of drugs listed in Schedules I and II. All CS III drugs have a currently accepted medical use in the United States."/>
    </extension>
    <code value="III"/>
    <display value="Schedule III"/>
    <definition value="Schedule III"/>
