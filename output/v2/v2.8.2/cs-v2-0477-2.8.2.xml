  <concept id="1">
    <extension url="http://hl7.org/fhir/StructureDefinition/codesystem-comments">
      <valueString value="Includes drugs that have a high potential for abuse, no currently accepted medical use in the United States and a lack of accepted safety for use under medical supervision."/>
