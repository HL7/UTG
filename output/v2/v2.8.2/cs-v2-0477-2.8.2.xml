  </concept>
  <concept id="2">
    <extension url="http://hl7.org/fhir/StructureDefinition/codesystem-comments">
      <valueString value="Includes drugs having currently accepted medical use in the United States and a high abuse potential, with severe psychological or physical dependence liability."/>
    </extension>
    <code value="II"/>
    <display value="Schedule II"/>
    <definition value="Schedule II"/>
    <property>
