  <description value="Types of combining results from a body of evidence (eg. summary data meta-analysis)."/>
  <caseSensitive value="true"/>
  <valueSet value="http://terminology.hl7.org/ValueSet/synthesis-type"/>
  <content value="complete"/>
  <concept>
    <code value="std-MA"/>
    <display value="summary data meta-analysis"/>
    <definition value="A meta-analysis of the summary data of estimates from individual studies or data sets."/>
  </concept>
  <concept>
    <code value="IPD-MA"/>
    <display value="individual patient data meta-analysis"/>
    <definition value="A meta-analysis of the individual participant data from individual studies or data sets."/>
  </concept>
  <concept>
    <code value="indirect-NMA"/>
    <display value="indirect network meta-analysis"/>
    <definition value="An indirect meta-analysis derived from 2 or more direct comparisons in a network meta-analysis."/>
  </concept>
  <concept>
    <code value="combined-NMA"/>
    <display value="combined direct plus indirect network meta-analysis"/>
    <definition value="An composite meta-analysis derived from direct comparisons and indirect comparisons in a network meta-analysis."/>
  </concept>
  <concept>
    <code value="range"/>
    <display value="range of results"/>
    <definition value="A range of results across a body of evidence."/>
  </concept>
  <concept>
    <code value="classification"/>
    <display value="classifcation of results"/>
    <definition value="An approach describing a body of evidence by categorically classifying individual studies (eg 3 studies showed beneft and 2 studied found no effect)."/>
  </concept>
  <concept>
    <code value="pooled"/>
    <display value="pooled rates"/>
    <definition value="An unweighted average."/>
  </concept>
