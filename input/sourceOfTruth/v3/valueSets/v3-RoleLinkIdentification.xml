<?xml version="1.0" encoding="UTF-8"?>

<ValueSet xmlns="http://hl7.org/fhir">
  <id value="v3-RoleLinkIdentification"/>
  <language value="en"/>
  <url value="http://terminology.hl7.org/ValueSet/v3-RoleLinkIdentification"/>
  <identifier>
    <system value="urn:ietf:rfc:3986"/>
    <value value="urn:oid:2.16.840.1.113883.1.11.20183"/>
  </identifier>
  <version value="2.0.0"/>
  <name value="RoleLinkIdentification"/>
  <title value="RoleLinkIdentification"/>
  <status value="active"/>
  <experimental value="false"/>
  <date value="2014-03-26"/>
  <description value="**Definition:** The source role provides identification for the target role. The source role must be IDENT. The player entity of the source role is constrained to be the same as the player of the target role if present. If the player is absent from the source role, then it is assumed to be the same as the player of the target role."/>
  <immutable value="true"/>
  <compose>
    <include>
      <system value="http://terminology.hl7.org/CodeSystem/v3-RoleLinkType"/>
      <filter>
        <property value="concept"/>
        <op value="is-a"/>
        <value value="IDENT"/>
      </filter>
    </include>
  </compose>
</ValueSet>