          <code value="internalId"/>
          <valueCode value="10017"/>
        </property>
      </concept>
    </concept>
    <concept>
      <code value="RQ"/>
      <display value="Required"/>
      <definition value="The message element must appear every time the message is communicated, but the value may be communicated as null."/>
